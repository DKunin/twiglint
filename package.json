    "linter",
    "cli"
  ],
  "license": "MIT",
  "devDependencies": {
    "ava": "0.22.0",
    "tokenizer2": "2.0.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "meow": "3.7.0",
    "text-table": "~0.2.0",
    "twig-ast": "git+ssh://git@github.com:DKunin/twig.js.git"
  }
}
