{
  "name": "twiglint",
  "description": "Linter for the twig templates",
  "homepage": "https://github.com/dkunin/twiglint",
  "version": "0.0.11",
  "repository": {
    "type": "git",
    "url": "git://github.com/dkunin/twiglint.git"
  },
  "bugs": {
    "url": "https://github.com/dkunin/twiglint/issues"
  },
  "private": false,
  "main": "index.js",
  "bin": {
    "twiglint": "./index.js"
  },
  "scripts": {
    "lint": "eslint **/*.js, ./*.js --fix",
